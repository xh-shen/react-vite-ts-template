{
	"name": "react-vite-ts-template",
	"version": "0.0.1",
	"description": "A basic template based on react vite typescript",
	"repository": {
		"type": "git",
		"url": "https://github.com/xh-shen/react-vite-ts-template.git"
	},
	"keywords": [
		"react",
		"react18",
		"typescript",
		"vite"
	],
	"author": "shen <9509394@qq.com>",
	"license": "MIT",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"build:staging": "tsc && vite build --mode staging",
		"report": "cross-env REPORT=true npm run build",
		"preview": "vite preview",
		"format": "prettier --write .",
		"lint": "eslint . --ext .js,.ts,.jsx,.tsx,.json --max-warnings 0 && pretty-quick --check --branch dev",
		"lint:fix": "eslint --fix . --ext .js,.ts,.jsx,.tsx,.json && pretty-quick --branch dev",
		"lint:commit": "commitlint --from $(git merge-base origin/dev HEAD) --to HEAD > ./commit-lint.txt",
		"lint:stylelint": "stylelint --cache --fix \"**/*.{less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
		"prepare": "husky install"
	},
	"dependencies": {
		"@ant-design/icons": "^4.7.0",
		"@ant-design/pro-components": "^2.3.19",
		"@reduxjs/toolkit": "^1.8.5",
		"ahooks": "^3.7.1",
		"antd": "^4.23.2",
		"axios": "^0.27.2",
		"classnames": "^2.3.2",
		"darkreader": "^4.9.58",
		"dayjs": "^1.11.5",
		"echarts": "^5.3.3",
		"i18next": "^21.9.2",
		"lodash-es": "^4.17.21",
		"nprogress": "^0.2.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-i18next": "^11.18.6",
		"react-redux": "^8.0.4",
		"react-router-dom": "^6.4.1",
		"react-transition-group": "^4.4.5",
		"type-fest": "^3.1.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.1.2",
		"@commitlint/config-conventional": "^17.1.0",
		"@esbuild-kit/cjs-loader": "^2.3.3",
		"@types/classnames": "^2.3.1",
		"@types/lodash-es": "^4.17.6",
		"@types/mockjs": "^1.0.6",
		"@types/node": "^18.7.18",
		"@types/nprogress": "^0.2.0",
		"@types/react": "^18.0.17",
		"@types/react-dom": "^18.0.6",
		"@types/react-redux": "^7.1.24",
		"@types/react-transition-group": "^4.4.5",
		"@types/rollup-plugin-visualizer": "^4.2.1",
		"@typescript-eslint/eslint-plugin": "^5.38.0",
		"@typescript-eslint/parser": "^5.38.0",
		"@vitejs/plugin-legacy": "^2.2.0",
		"@vitejs/plugin-react": "^2.1.0",
		"commitizen": "^4.2.5",
		"cross-env": "^7.0.3",
		"cz-git": "^1.3.11",
		"czg": "^1.3.11",
		"eslint": "^8.23.1",
		"eslint-config-prettier": "^8.5.0",
		"eslint-define-config": "^1.7.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.31.8",
		"eslint-plugin-react-hooks": "^4.6.0",
		"fast-glob": "^3.2.12",
		"husky": "^8.0.1",
		"less": "^4.1.3",
		"lint-staged": "^13.0.3",
		"mockjs": "^1.1.0",
		"postcss": "^8.4.16",
		"postcss-html": "^1.5.0",
		"postcss-less": "^6.0.0",
		"postcss-preset-env": "^7.8.2",
		"prettier": "^2.7.1",
		"pretty-quick": "^3.1.3",
		"rollup-plugin-visualizer": "^5.8.2",
		"stylelint": "^14.12.0",
		"stylelint-config-prettier": "^9.0.3",
		"stylelint-config-recess-order": "^3.0.0",
		"stylelint-config-recommended": "^9.0.0",
		"stylelint-config-standard": "^28.0.0",
		"stylelint-less": "^1.0.6",
		"stylelint-order": "^5.0.0",
		"typescript": "^4.6.4",
		"vite": "^3.1.0",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-eslint": "^1.8.1",
		"vite-plugin-html": "^3.2.0",
		"vite-plugin-imagemin": "^0.6.1",
		"vite-plugin-mock": "^2.9.6",
		"vite-plugin-svg-icons": "^2.0.1"
	},
	"engines": {
		"node": ">= 16"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-git"
		}
	},
	"lint-staged": {
		"*.{js,ts,jsx,tsx,json}": "eslint --fix"
	}
}
