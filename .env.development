# env
NODE_ENV = 'development'

# Whether to open mock
VITE_USE_MOCK = true

# setting
VITE_ENABLE_SETTING = true

# public path
VITE_PUBLIC_PATH = /

# Cross-domain proxy, you can configure multiple
# Please note that no line breaks
VITE_PROXY = [["/api","http://localhost:3000"],["/upload","http://localhost:3300/upload"]]

# Delete console
VITE_DROP_CONSOLE = false

# Basic interface address prefix
VITE_API_URL_PREFIX=/api

# File upload address prefix
VITE_UPLOAD_URL_PREFIX=/upload

